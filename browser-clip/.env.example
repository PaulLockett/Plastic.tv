# Browser Clip Test Environment Configuration

# =============================================================================
# Browserbase Configuration (for E2E and AI tests)
# =============================================================================

# Your Browserbase API key (get it from https://www.browserbase.com/dashboard)
BROWSERBASE_API_KEY=your_browserbase_api_key

# Your Browserbase project ID
BROWSERBASE_PROJECT_ID=your_project_id

# Extension ID after uploading (set by upload-extension.ts script)
BROWSERBASE_EXTENSION_ID=

# =============================================================================
# Google AI Configuration (for AI agent tests with Gemini)
# =============================================================================

# Google API key for Stagehand AI agent tests (uses Gemini models)
GOOGLE_API_KEY=your_google_api_key

# =============================================================================
# Test Configuration
# =============================================================================

# Set to 'true' to run tests against Browserbase cloud instead of local Chrome
BROWSERBASE_ENABLED=false

# Enable verbose test output
VERBOSE_TESTS=false

# =============================================================================
# Supabase Configuration (optional, for full integration tests)
# =============================================================================

# Test Supabase project URL
TEST_SUPABASE_URL=https://your-test-project.supabase.co

# Test Supabase anon key
TEST_SUPABASE_KEY=your_test_anon_key
